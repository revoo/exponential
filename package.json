  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0"
  },
  "build": {
    "appId": "com.revoo.exponential",
    "productName": "Exponential 1.0.0",
    "copyright": "Co",
    "mac": {
